{
  "name": "andrevandal.dev",
  "version": "1.0.0",
  "description": "The personal website, blog, portifolio and online CV of AndrÃ© Van Dal, a web developer.",
  "author": "AndrÃ© Van Dal &lt;me@andrevandal.dev&gt;",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint": "npm-run-all --parallel lint:js lint:css",
    "lint:fix": "npm-run-all --parallel lint:js:fix lint:css:fix",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:css": "stylelint '**/*.{vue,htm,html,css,sss,less,scss,sass,postcss}' --ignore-path .gitignore",
    "lint:css:fix": "yarn lint:css --fix",
    "release": "standard-version",
    "release:patch": "yarn release --release-as patch",
    "release:minor": "yarn release --release-as minor",
    "release:major": "yarn release --release-as major",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint:js",
    "*.{css,vue}": "yarn lint:css"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": {
    "production": [
      ">0.25%",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "modern": [
      "last 1 chrome version",
      "last 1 firefox version"
    ],
    "ssr": [
      "node 12"
    ]
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "1.0.0",
    "@nuxtjs/axios": "5.12.2",
    "@nuxtjs/pwa": "3.0.2",
    "@tailwindcss/typography": "^0.2.0",
    "nuxt": "2.14.4",
    "tailwindcss-typography": "^3.1.0"
  },
  "devDependencies": {
    "@babel/runtime-corejs3": "7.11.2",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@nuxt/babel-preset-app": "2.14.4",
    "@nuxt/types": "2.14.4",
    "@nuxt/typescript-build": "2.0.2",
    "@nuxtjs/eslint-config-typescript": "3.0.0",
    "@nuxtjs/eslint-module": "2.0.0",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/svg": "0.1.12",
    "@nuxtjs/tailwindcss": "3.0.2",
    "@types/node": "14.6.2",
    "@vue/test-utils": "1.0.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "26.3.0",
    "core-js": "3",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "^4.0.0",
    "jest": "26.4.2",
    "lint-staged": "10.2.13",
    "npm-run-all": "^4.1.5",
    "postcss-nested": "^4.2.3",
    "prettier": "2.1.1",
    "standard-version": "9.0.0",
    "stylelint": "13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "ts-jest": "26.3.0",
    "vue-jest": "5.0.0-alpha.3"
  }
}
